{
  "name": "@jamiemagee/ghsa-offline-monorepo",
  "private": true,
  "workspaces": [
    "./packages/*"
  ],
  "version": "0.0.0-semantic-release",
  "repository": {
    "type": "git",
    "url": "https://github.com/JamieMagee/ghsa-offline.git"
  },
